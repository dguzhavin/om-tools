const ENV = {
    CORE: "core_regression solver_v1.1",
    DATA:{
        NAME_MC: "---Мультикуб - источник данных---",   //мультикуб с источником данных
        VIEW: "---Вью мк с данными---",  //вью источника данных, кубы в колонках, измерения в строках
        FILTER: "---Булеан куб с фильтром---"  //фильтр ячеек, участвующих в расчете
    },
    PARAMS:{
        REGRESSION_TYPE: "QUADRATIC",    //LINEAR, QUADRATIC  - тип рассчитываемой регрессии
        PREDICTOR: "---Куб с независимой переменной---",     //X1 - независимая переменная (предиктор), в v1.1 доступна только одна, для нескольких нужна реализация множественной регрессии
        DEPENDANT: "---Куб с зависимой переменной---",       //Y - зависимая переменная (отклик)
    },
    SOLVE_RESULT:{
        MULTICUBE: "---Мультикуб для результата---",  //мультикуб для записи результатов вычисления
        COEFFS: {
            A:"---коэффициент A---",  //первый к-т уравнения
            B:"---коэффициент B---",  //второй к-т уравнения
            C:"---коэффициент C---"  //третий к-т уравнения
        },     //кубы для записи к-тов - a, b, c...
        EQUATION: "---куб для записи уравнения---",  //текстовый куб для записи уравнения
        WRITE_STATISTICAL_INDICATORS: "TRUE",   //TRUE-FALSE для вычисления  R (достовеность аппроксимации)
        R_SQUARED: "---куб для записи R---"
    }
};

om.common.resultInfo()
    .actionsInfo()
    .makeMacrosAction(ENV.CORE)
    .appendAfter()
    .environmentInfo()
    .set('ENV', ENV);
